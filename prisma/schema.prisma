generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["analytics", "public"]
}

model d_athletes {
  athlete_uuid                   String                    @id @db.VarChar(36)
  name                           String
  normalized_name                String                    @unique
  date_of_birth                  DateTime?                 @db.Date
  age                            Decimal?                  @db.Decimal
  age_at_collection              Decimal?                  @db.Decimal
  gender                         String?
  height                         Decimal?                  @db.Decimal
  weight                         Decimal?                  @db.Decimal
  notes                          String?
  source_system                  String?
  source_athlete_id              String?
  app_db_uuid                    String?
  app_db_synced_at               DateTime?                 @db.Timestamptz(6)
  email                          String?
  has_pitching_data              Boolean                   @default(false)
  has_athletic_screen_data       Boolean                   @default(false)
  has_pro_sup_data               Boolean                   @default(false)
  has_readiness_screen_data      Boolean                   @default(false)
  has_mobility_data              Boolean                   @default(false)
  has_proteus_data               Boolean                   @default(false)
  has_hitting_data               Boolean                   @default(false)
  pitching_session_count         Int                       @default(0)
  athletic_screen_session_count  Int                       @default(0)
  pro_sup_session_count          Int                       @default(0)
  readiness_screen_session_count Int                       @default(0)
  mobility_session_count         Int                       @default(0)
  proteus_session_count          Int                       @default(0)
  hitting_session_count          Int                       @default(0)
  created_at                     DateTime                  @default(now()) @db.Timestamptz(6)
  updated_at                     DateTime                  @default(now()) @db.Timestamptz(6)
  age_group                      String?
  has_arm_action_data            Boolean                   @default(false)
  arm_action_session_count       Int                       @default(0)
  has_curveball_test_data        Boolean                   @default(false)
  curveball_test_session_count   Int                       @default(0)
  has_pitching_trial_data        Boolean?                  @default(false)
  pitching_trial_count           Int?                      @default(0)
  source_athlete_map             source_athlete_map[]
  f_arm_action                   f_arm_action[]
  f_athletic_screen              f_athletic_screen[]
  f_athletic_screen_cmj          f_athletic_screen_cmj[]
  f_athletic_screen_dj           f_athletic_screen_dj[]
  f_athletic_screen_nmt          f_athletic_screen_nmt[]
  f_athletic_screen_ppu          f_athletic_screen_ppu[]
  f_athletic_screen_slv          f_athletic_screen_slv[]
  f_curveball_test               f_curveball_test[]
  f_hitting_trials               f_hitting_trials[]
  f_kinematics_hitting           f_kinematics_hitting[]
  f_kinematics_pitching          f_kinematics_pitching[]
  f_mobility                     f_mobility[]
  f_pitching_trials              f_pitching_trials[]
  f_pro_sup                      f_pro_sup[]
  f_proteus                      f_proteus[]
  f_readiness_screen             f_readiness_screen[]
  f_readiness_screen_cmj         f_readiness_screen_cmj[]
  f_readiness_screen_i           f_readiness_screen_i[]
  f_readiness_screen_ir90        f_readiness_screen_ir90[]
  f_readiness_screen_ppu         f_readiness_screen_ppu[]
  f_readiness_screen_t           f_readiness_screen_t[]
  f_readiness_screen_y           f_readiness_screen_y[]

  @@index([app_db_uuid], map: "idx_d_athletes_app_db_uuid")
  @@index([normalized_name], map: "idx_d_athletes_normalized_name")
  @@index([source_system, source_athlete_id], map: "idx_d_athletes_source_system")
  @@schema("analytics")
}

model source_athlete_map {
  source_system     String
  source_athlete_id String
  athlete_uuid      String     @db.VarChar(36)
  created_at        DateTime   @default(now()) @db.Timestamptz(6)
  d_athletes        d_athletes @relation(fields: [athlete_uuid], references: [athlete_uuid], onDelete: Cascade, map: "DSourceAthleteMap_athleteUuid_fkey")

  @@id([source_system, source_athlete_id])
  @@index([source_system], map: "idx_source_map_system")
  @@index([athlete_uuid], map: "idx_source_map_uuid")
  @@schema("analytics")
}

model f_arm_action {
  id                          Int        @id @default(autoincrement())
  athlete_uuid                String     @db.VarChar(36)
  session_date                DateTime   @db.Date
  source_system               String     @default("arm_action") @db.VarChar(50)
  source_athlete_id           String?    @db.VarChar(100)
  filename                    String?
  movement_type               String?
  foot_contact_frame          Int?
  release_frame               Int?
  age_at_collection           Decimal?   @db.Decimal
  age_group                   String?
  created_at                  DateTime   @default(now()) @db.Timestamp(6)
  arm_abduction_at_footplant  Decimal?   @db.Decimal
  max_abduction               Decimal?   @db.Decimal
  shoulder_angle_at_footplant Decimal?   @db.Decimal
  max_er                      Decimal?   @db.Decimal
  arm_velo                    Decimal?   @db.Decimal
  max_torso_rot_velo          Decimal?   @db.Decimal
  torso_angle_at_footplant    Decimal?   @db.Decimal
  score                       Decimal?   @db.Decimal
  d_athletes                  d_athletes @relation(fields: [athlete_uuid], references: [athlete_uuid], onDelete: Cascade, map: "FArmAction_athleteUuid_fkey")

  @@index([session_date], map: "idx_f_arm_action_date")
  @@index([movement_type], map: "idx_f_arm_action_movement_type")
  @@index([athlete_uuid], map: "idx_f_arm_action_uuid")
  @@schema("public")
}

model f_athletic_screen {
  id                Int        @id @default(autoincrement())
  athlete_uuid      String     @db.VarChar(36)
